<div class="modal fade" id="cronograma" tabindex="-1" role="dialog" aria-labelledby="cronograma" aria-hidden="true">
  <div class="modal-dialog">
    <div class="modal-content">
      <div class="modal-header text-dark">
        <h5 class="modal-title" id="exampleModalLabel">Adicione atividade ao seu Cronograma</h5>
        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
          <span aria-hidden="true">&times;</span>
        </button>
      </div>
      <div class="modal-body text-dark">
        <form action="/authUser/registerAtividade" method="POST">
            <div class="form-group">
              <small>Inicio da atividade: </small>
              <input class="col-auto" type="time" id="horarioInicial" name="horarioInicial" min="00:00" max="23:59" required>
              <small>Fim da atividade: </small>
              <input class="col-auto" type="time" id="horarioTermino" name="horarioTermino" min="00:00" max="23:59" required>
              <label for="validationServer01">Informe o que vai fazer nesse horário:</label>
                <input type="text" class="form-control is-valid" name="atividade" placeholder="O que vai fazer neste horário" id="atividade" required>
              <div class="col-auto my-1">
                <label for="validationServer01">Informe o dia da semana</label>
                <select class="custom-select is-valid" name="dia" size="3" required>
                  <option value="segunda">Segunda</option>
                  <option value="terca">Terça</option>
                  <option value="quarta">Quarta</option>
                  <option value="quinta">Quinta</option>
                  <option value="sexta">Sexta</option>
                  <option value="sabado">Sábado</option>
                  <option value="domingo">Domingo</option>
                </select>
              </div>
            </div>
          <button type="submit" class="btn btn-info col-auto">Salvar</button>
          <button type="button" class="btn btn-secondary text-center" data-dismiss="modal">Cancelar</button>
        </form>
      </div>
    </div>
  </div>
